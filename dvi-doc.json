{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Document",
    "definitions": {
        "Document": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "fonts": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Font"
                    }
                },
                "pages": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Page"
                    }
                }
            },
            "required": [
                "fonts",
                "pages"
            ],
            "title": "Document"
        },
        "Font": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "fontNum": {
                    "type": "integer"
                },
                "fontName": {
                    "type": "string"
                },
                "fontPath": {
                    "type": "string"
                },
                "fontFeatures": {
                    "type": "string"
                }
            },
            "required": [
                "fontFeatures",
                "fontName",
                "fontNum",
                "fontPath"
            ],
            "title": "Font"
        },
        "Page": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "pageFonts": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/PageFont"
                    }
                },
                "rules": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Rule"
                    }
                },
                "images": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Image"
                    }
                }
            },
            "required": [
                "images",
                "pageFonts",
                "rules"
            ],
            "title": "Page"
        },
        "Image": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "fileName": {
                    "type": "string"
                },
                "llx": {
                    "type": "integer"
                },
                "lly": {
                    "type": "integer"
                },
                "urx": {
                    "type": "integer"
                },
                "ury": {
                    "type": "integer"
                },
                "rwi": {
                    "type": "integer"
                }
            },
            "required": [
                "fileName",
                "llx",
                "lly",
                "rwi",
                "urx",
                "ury"
            ],
            "title": "Image"
        },
        "PageFont": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "fontNum": {
                    "type": "integer"
                },
                "glyphs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Glyph"
                    }
                }
            },
            "required": [
                "fontNum",
                "glyphs"
            ],
            "title": "PageFont"
        },
        "Glyph": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "glyphIndex": {
                    "type": "integer"
                },
                "glyphSizes": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/GlyphSize"
                    }
                }
            },
            "required": [
                "glyphIndex",
                "glyphSizes"
            ],
            "title": "Glyph"
        },
        "GlyphSize": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "sz": {
                    "type": "integer"
                },
                "glyphPlacements": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/GlyphPlacement"
                    }
                }
            },
            "required": [
                "glyphPlacements",
                "sz"
            ],
            "title": "GlyphSize"
        },
        "GlyphPlacement": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "x": {
                    "type": "integer"
                },
                "y": {
                    "type": "integer"
                }
            },
            "required": [
                "x",
                "y"
            ],
            "title": "GlyphPlacement"
        },
        "Rule": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "x": {
                    "type": "integer"
                },
                "y": {
                    "type": "integer"
                },
                "w": {
                    "type": "integer"
                },
                "h": {
                    "type": "integer"
                }
            },
            "required": [
                "h",
                "w",
                "x",
                "y"
            ],
            "title": "Rule"
        }
    }
}
